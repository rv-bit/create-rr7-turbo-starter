{
	"build": {
		"builder": "DOCKERFILE",
		"dockerfilePath": "apps/web-admin/Dockerfile",
		"context": ".",
		"watchPatterns": ["apps/web-admin/**", "packages/**", "turbo.json", "package.json"]
	},
	"deploy": {
		"numReplicas": 1,
		"restartPolicyType": "ON_FAILURE",
		"sleepApplication": false
	}
}
